    "# [8'] A 3d time-space grid for subproblem LSA"
   ]
  },
  {
   "cell_type": "markdown",
   "id": "a13cc0e1",
   "metadata": {},
   "source": [
    "See my description document at "Subproblem LSA - 3d version.pdf"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 290,
   "id": "cc9d44fc",
   "metadata": {
    "scrolled": true
   },
   "outputs": [
    {
     "data": {
      "text/plain": [
       "sp_3d (generic function with 2 methods)"
      ]
     },
     "execution_count": 290,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "function sp_3d(n, global_time, travel_distance, travel_time, windows, load, rho_v, pi_v, mu_v, cool_stuff=false)\n",
    "    N = [ [[1, 1]] ] \n",
    "    R = [-rho_v]  \n",
    "    L = [[1, 1]]  \n",
    "    A = [true] \n",
    "    \n",
    "    current_states = []\n",
    "    total_states = []\n",
    "    \n",
    "    current_state = 1\n",
    "    total_state = 1\n",
    "    \n",
    "    while 1==1\n",
    "        \n",
    "        # STEP 1: Check whether we want to move on to checking the next path\n",
    "        if (L[current_state][1] == n+2) | (L[current_state][2] >= global_time) | (~A[current_state])\n",
    "            current_state += 1\n",
    "            if current_state > total_state\n",
    "                print(current_state, \" \", total_state, \" nothing more to check\")\n",
    "                break\n",
    "            else\n",
    "                continue\n",
    "            end\n",
    "        end\n",
    "        \n",
    "        # STEP 2: Enter the for loop.\n",
    "        for i in 2:(n+2)\n",
    "            for t in L[current_state][2]+1:global_time\n",
    "            \n",
    "                if cool_stuff\n",
    "                    push!(current_states, current_state)\n",
    "                    push!(total_states, total_state)\n",
    "                end\n",
    "\n",
    "                #= STEP 2-1: Check we haven't visited i already.\n",
    "                This will be a bit different from the previous implementation.\n",
    "                We KNOW that there will be no struggle with time, because it keeps on \n",
    "                increasing. But we must make sure we never go back to visiting a place\n",
    "                we already visited.\n",
    "                =#\n",
    "                if ~(i in [node[1] for node in N[current_state]])\n",
    "\n",
    "                    #= STEP 2-2: Feasibility check. Three things.\n",
    "\n",
    "                    1) We need enough time to work if we arrive here. So t + new_job_required_time must\n",
    "                    be less than or equal to new_job_window_close.\n",
    "\n",
    "                    2) We need to arrive late enough to be within the window. So\n",
    "                    new_window_start <= t <= new_window_end. The second part is redundant\n",
    "                    because 1) already encapsulates it. Therefore, we need the first part.\n",
    "\n",
    "                    3) We need this tsn to be actually arrivable. Specifically we need\n",
    "                    enough time to get from the old time to this time.\n",
    "                    \n",
    "                    4) Don't run away from a previous job. Make sure \n",
    "                    travel_time + old_time + old_job_load <= t.\n",
    "                    =#\n",
    "                    if L[current_state][1] != 1\n",
    "                        enough_time_to_work = (t + load[i-1] <= windows[i-1][2])\n",
    "                        arrive_within_pocket = (windows[i-1][1] <= t)\n",
    "                        arrivable_time = (travel_time[L[current_state][1], i] + L[current_state][2] <= t)\n",
    "                        no_run_away = (travel_time[L[current_state][1], i] + L[current_state][2] + load[L[current_state][1]-1] <= t)\n",
    "\n",
    "                        if ~(enough_time_to_work && arrive_within_pocket && arrivable_time && no_run_away)\n",
    "                            continue\n",
    "                        end\n",
    "                    end\n",
    "\n",
    "                    # STEP 2-3-1: We did not create an invalid path earlier\n",
    "                    if length(N) < total_state + 1\n",
    "\n",
    "                        #modify N\n",
    "                        push!(N, copy(N[current_state]))\n",
    "                        push!(N[total_state+1], [i, t])\n",
    "\n",
    "                        # Cost.\n",
    "                        # ADD the segment distance.\n",
    "                        add_segdist = travel_distance[L[current_state][1], i]\n",
    "                        # SUBTRACT pi_i.\n",
    "                        subtract_pi = i < n+2 ? -pi_v[i-1] : 0\n",
    "                        # SUBTRACT mu_it.\n",
    "                        subtract_mu = i < n+2 ? -mu_v[i-1, t] : 0\n",
    "\n",
    "                        push!(R, R[current_state] + add_segdist + subtract_pi + subtract_mu)\n",
    "\n",
    "                        # Last node is updated.\n",
    "                        push!(L, [i, t])\n",
    "\n",
    "                        # Update feasibility.\n",
    "                        push!(A, true)\n",
    "\n",
    "                    # STEP 2-3-2: We created an invalid path earlier\n",
    "                    else\n",
    "                        # Change the path\n",
    "                        N[total_state + 1] = N[current_state]\n",
    "                        push!(N[total_state+1], [i, t])\n",
    "\n",
    "                        # Change the cost - watch out for if we're going back to depot on pi and mu!\n",
    "                        add_segdist = travel_distance[L[current_state][1], i]\n",
    "                        subtract_pi = i < n+2 ? -pi_v[i-1] : 0\n",
    "                        subtract_mu = i < n+2 ? -mu_v[i-1, t] : 0\n",
    "                        R[total_state + 1] = R[current_state] + add_segdist + subtract_pi + subtract_mu\n",
    "\n",
    "                        # Change the last node\n",
    "                        L[total_state+1] = [i, t]\n",
    "\n",
    "                        # Change the feasibility\n",
    "                        A[total_state+1] = true\n",
    "                    end \n",
    "\n",
    "                    # STEP 2-4: Reviewing true Bellman-Ford step: if a path is totally dominated by another.\n",
    "                    NDom = true\n",
    "\n",
    "                    #= Iterate over all old paths and check if they have the same start (guaranteed)\n",
    "                    and end point. If they do, check which cost is lower. =#\n",
    "                    for s in 1:total_state\n",
    "                        # Is the route feasible?\n",
    "                        if A[s]\n",
    "                            # if so, do they share nodes and end on the same one?\n",
    "                            if (issetequal(N[total_state+1], N[s])) & (L[total_state+1] == L[s])\n",
    "                                # New path is too bad, overwrite it later\n",
    "                                if R[total_state+1] > R[s]\n",
    "                                    NDom = false\n",
    "                                    break\n",
    "                                # Old path is too bad, declare it not valid and proceed to NDom add total state\n",
    "                                else\n",
    "                                    A[s] = false\n",
    "                                end\n",
    "                            end\n",
    "                        end\n",
    "                    end\n",
    "\n",
    "                    # New state good enough, it's set in stone and we add\n",
    "                    if NDom \n",
    "                        total_state += 1\n",
    "                    end\n",
    "                end\n",
    "            end\n",
    "        end\n",
    "        \n",
    "        current_state += 1 # concluded iteration for that state in mind. Now next one\n",
    "        \n",
    "        #= It's possible that we've just caught up to the final total state, meaning we didn't generate\n",
    "        anything and can stop here. =#\n",
    "        if current_state == total_state\n",
    "            print(current_state, \" \", total_state, \" came from bottom\")\n",
    "            break\n",
    "        end     \n",
    "    end\n",
    "    \n",
    "    #= It is not actually necessary to return L. This was a helpful parameter which we\n",
    "    embedded in order to aid computation. But if we needed to get L, we would just go to \n",
    "    the path of concern and extract last([path_of_concern]). =#\n",
    "    \n",
    "    if ~cool_stuff\n",
    "        return N, A, R, T\n",
    "    else\n",
    "        return N, A, R, T, current_states, total_states\n",
    "    end\n",
    "end"
   ]
  },
